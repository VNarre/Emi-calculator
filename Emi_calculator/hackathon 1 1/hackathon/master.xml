<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EMI_Calculator" thread-count="5" parallel="tests">
	<listeners>
		<listener class-name="Utilities.ExtentsReports"></listener>
	</listeners>
  <test  name="ChromeTest">
	<parameter name="os" value="Windows"></parameter>
	<parameter name="browser" value="chrome"></parameter>
    <classes>
      <class name="TestCase.BaseClass"/>
      <class name="TestCase.TS001_CarLoanTest"/>
      <class name="TestCase.TS002_HomeLoanTest"/>
      <class name="TestCase.TS003_UiValidation"/>
    </classes>
  </test>
  
   <test  name="EdgeTest">
	<parameter name="os" value="Windows"></parameter>
	<parameter name="browser" value="Edge"></parameter>
    <classes>
      <class name="TestCase.BaseClass"/>
      <class name="TestCase.TS001_CarLoanTest"/>
      <class name="TestCase.TS002_HomeLoanTest"/>
       <class name="TestCase.TS003_UiValidation"/>
    </classes>
  </test>   
</suite> <!-- Suite -->
